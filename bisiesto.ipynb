   "source": [
    "def is_leap(year):\n",
    "    leap = False\n",
    "    if(year%4==0):\n",
    "        if(year%100==0):\n",
    "            leap=False\n",
    "        if(year%400==0):\n",
    "            leap=True\n",
    "    else:\n",
    "        leap=False\n",
    "    return leap"
   ]
